@page "/item/getitembyid/{id:int}"
@using MAUILocal.Model
@using MAUILocal.Services
@inject MAUILocal.Services.IItemService db
<h1 style="text-align:center">@cat.prodname</h1>
<div>
<img class="hfimg" style="height:300px" src="@cat.imgur"/>
    <img class="hfimgrev" style="height:70px" src="data:image/jpeg;base64,"/>
</div>
<h4>About: </h4>
<p>@cat.description</p>
<h5><b>Price: Rs </b>@cat.price</h5>
<NavLink class="buttongreen" href="@($"cart/addtocart/{cat.id}")">Add To Cart</NavLink>

@code {
    [Parameter]
    public int id { get; set; }
    Items cat = new Items();
    protected override void OnInitialized()
    {
        cat = db.getItemById(id);
    }
}
